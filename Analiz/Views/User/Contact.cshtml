                        </a>
                    </li>
                    <li class="mdi-view-column">
                        <a href="@Url.Action("Index","Home")">
                            <i class="fa fa-home"></i> <span>Ana Sayfa</span>
                        </a>
                    </li>
                    <li class="mdi-view-column">
                        <a class="btn-danger" href="@Url.Action("Exit","User")">
                            <i class="fa fa-times"></i>
                            <span style="color:white;">Çıkış</span>
                        </a>
                    </li>

                </ul>
            </section>
            <!-- /.sidebar -->
        </aside>
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <h1>
                    İletişim
                </h1>
                <ol class="breadcrumb">
                    <li><a href="#"><i class="fa fa-dashboard"></i> Dashboard</a></li>
                    <li class="active">İletişim</li>
                </ol>
            </section>
            <!-- Main content -->
            <section class="content">
                <div class="row">
                    <div class="col-md-3">
                        <!-- Profile Image -->
                        <!-- /.box -->
                        <!-- About Me Box -->
                        <!-- /.box -->
                    </div>
                    <!-- /.col -->
                    <div class="col-md-12">
                        <div class="nav-tabs-custom">
                            <ul class="nav nav-tabs">
                                <li class="active"><a href="#settings" data-toggle="tab">Mesajınız</a></li>
                            </ul>
                            <!-- /.tab-pane -->
                            <!-- /.tab-pane -->
                            <div class="tab-pane" id="settings">
                                <form class="form-horizontal" asp-action="Contact2" method="post">
                                    <div class="form-group">
                                        <label asp-for="Kategori" class="col-sm-2 control-label">Kategori Seçiniz</label>
                                        <div class="col-sm-10">

                                            <select asp-for="Kategori" class="burç" id="cars" required>
                                                <option value="Seçiniz">Seçiniz</option>
                                                <option value="İstek">İstek</option>
                                                <option value="Öneri">Öneri</option>
                                                <option value="Şikayet">Şikayet</option>
                                                <option value="Yardım">Yardım</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="inputName" class="col-sm-2 control-label">Mesajınızı Yazınız</label>
                                        <div class="col-sm-10">
                                            <textarea asp-for="Mesaj" type="text" class="form-control" id="inputName" required></textarea>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label style="margin-left:17%; color:red;" class="col-sm-8">Mesajınız tarafımıza ulaştığı anda en kısa sürede tarafınıza çözüm iletilecektir.</label>
                                    </div>
                                    <div class="form-group ">
                                        <label for="inputName" class="col-sm-2 control-label"></label>
                                        <div class="col-sm-10">
                                            <button type="submit" class="col-lg-12 col-xs-12 btn-instagram ">Gönder <i class="fa fa-user-plus"></i></button>
