                                <li><a>@Html.ActionLink("Hakkımızda", "About", "Home", new { })</a></li>
                                <li><a>@Html.ActionLink("Analiz Yaptır", "Login", "Home", new { })</a></li>
                                <li><a>@Html.ActionLink("Üye Ol", "SıngUp", "Home", new { })</a></li>
                                <li><a>@Html.ActionLink("Blog Yazıları", "BlogList", "Home", new { })</a></li>
                                <li><a>@Html.ActionLink("Yardım", "Help", "Home", new { })</a></li>
                            </ul>
                        </div>
                        <div class="col-sm-5 col-lg-9 col-xl-2">
                            <h4 class="footer-classic-title">İletişim</h4>
                            <div class="row row-20 row-sm-35">
